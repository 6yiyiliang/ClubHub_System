/* components/Navigation/index.wxss */
.navigation-wrap{ width: 100%; margin-top: auto;}
.fixed-nav-wrap {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 15rpx; /* 将 bottom 值从 0 改为 20rpx，上移导航栏 */
    z-index: 100;
  }
.fixed-nav-wrap .shadow-box{ 
  width: 100%; 
  min-height: 100rpx; 
  background-color: #ffffff;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, .2); 
  position: relative; 
  z-index: 1; 
}
.fixed-nav-wrap .shadow-box .middle-circle{ 
  width: 150rpx; 
  height: 150rpx; 
  margin-left: -75rpx;
  border-radius: 100rpx; 
  box-shadow: 0 0 10rpx rgba(0, 0, 0, .2); 
  background-color: #ffffff;
  position: absolute; 
  left: 50%; 
  top: -45rpx; 
  z-index: 1; 
}
.navigation-empty-wrap{ width: 100%; }
.navigation-empty-wrap .empty-box{ width: 100%; height: 100rpx; }
.bottom-safet-area{ background-color: #FFF; }
.fixed-nav-content{ 
  background-color: #ffffff; 
  width: 100%; 
  height: 120rpx; 
  position: absolute; 
  left: 0; 
  top: 0; 
  z-index: 5; 
}
.fixed-nav-content .table{ display: table; width: 100%; height: 100rpx; font-size: 26rpx; }
.fixed-nav-content .table .cell{ display: table-cell; vertical-align: middle; width: 1%; text-align: center;}
.fixed-nav-content .table .cell.active{ color: #93D2F3; }
.icon {
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin: 0 auto 10rpx auto; 
}
.large-icon {
  position: relative;
  width: 70rpx;
  height: 70rpx;
  top: -20rpx;
  margin-bottom: -10rpx
}